{
  "family": "health-api-task-family",
  "taskRoleArn": "arn:aws:iam::336449586386:role/ecsTaskExecutionRole",
  "executionRoleArn": "arn:aws:iam::336449586386:role/ecsTaskExecutionRole",
  "networkMode": "awsvpc",
  "cpu": "256",
  "memory": "512",
  "containerDefinitions": [
    {
      "name": "health-api-container",
      "image": "336449586386.dkr.ecr.us-east-1.amazonaws.com",
      "essential": true,
      "portMappings": [
        {
          "containerPort": 8080,
          "hostPort": 8080,
          "protocol": "tcp"
        }
      ],
      "environment": [
        {
          "name": "SPRING_PROFILES_ACTIVE",
          "value": "production"
        }
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "ecs",
          "awslogs-region": "us-east-1",
          "awslogs-stream-prefix": "ecs"
        }
      }
    }
  ],
  "requiresCompatibilities": [
    "FARGATE"
  ],
  "placementConstraints": []
}
